Algoritmo CienPrimeroParesB
	
	Definir numero Como Entero;
	numero <- 0;
	//ponemos 200 porque si pusieramos 100 nos mostraria por pantalla 50 numero pares//
	//al poner 200 nos imprime los 100 primero numeros pares//
	Mientras (numero < 200) Hacer;
		Escribir numero;	
		numero <- numero + 2;
	FinMientras
	
FinAlgoritmo
